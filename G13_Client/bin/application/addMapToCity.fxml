            <Font name="Comic Sans MS" size="12.0" />
         </font></TextField>
      <Button fx:id="addMapToCity" layoutX="441.0" layoutY="464.0" mnemonicParsing="false" onAction="#add" text="Add">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="path" alignment="CENTER" layoutX="352.0" layoutY="403.0" prefHeight="42.0" prefWidth="225.0" promptText="map location ">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></TextField>
      <Button fx:id="Path" layoutX="352.0" layoutY="377.0" mnemonicParsing="false" onAction="#Path_dir" prefHeight="32.0" prefWidth="225.0" text="Select To Enter Path">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
