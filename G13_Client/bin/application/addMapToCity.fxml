<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="568.0" prefWidth="858.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddMapToCity">
   <children>
      <ImageView fx:id="image" fitHeight="813.0" fitWidth="900.0" layoutX="-10.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../background.jpg" />
         </image></ImageView>
      <TextField fx:id="CityId" alignment="CENTER" layoutX="352.0" layoutY="182.0" prefHeight="32.0" prefWidth="225.0" promptText="Enter City Id">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></TextField>
      <Text layoutX="306.0" layoutY="122.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Map To City">
         <font>
            <Font name="Comic Sans MS" size="40.0" />
         </font>
      </Text>
      <Button layoutX="25.0" layoutY="23.0" mnemonicParsing="false" onAction="#back" text="back">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="MapId" alignment="CENTER" layoutX="349.0" layoutY="250.0" prefHeight="32.0" prefWidth="225.0" promptText="Enter Map Id">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></TextField>
      <TextField fx:id="description" alignment="CENTER" layoutX="352.0" layoutY="320.0" prefHeight="32.0" prefWidth="225.0" promptText="Enter Description To The New Map">
